{
  "compilerOptions": {
    "target": "es2022",
    "module": "node16",
    "outDir": "./lib",
    "strict": true,
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    // "noEmit": true,
    "checkJs": true,
    "declaration": true
  },
  "include": ["src/**/*"]
}
